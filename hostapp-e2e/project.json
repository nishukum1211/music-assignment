{
  "name": "hostapp-e2e",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "hostapp-e2e/src",
  "implicitDependencies": ["hostapp"],
  "targets": {
    "e2e": {
      "executor": "@nx/playwright:playwright",
      "outputs": ["{workspaceRoot}/dist/.playwright/hostapp-e2e"],
